let cor nested = () -> {
	cout "--start nested";
	yield "nested 1";
	yield "nested 2";
	yield "nested 3";
	cout "--end nested";
};

let cor c = x -> {
	cout "--start";
	yield "first";
	yield "second";
	yield "third";
	
	foreach (x in nested())
		yield x;
	
	cout "--end";	 
};

foreach (x in c(2))
	cout x;

