open Test
open BasicTests
open FunTests
open RangeTests
open ListTests
open TupleTests
open RecordTests
open VariantTests
open ComplexTests
open ComprehensionTests
open ThunkTests

let lst = [] 
	|> Test.regFuns BasicTests
    |> Test.regFuns FunTests
    |> Test.regFuns RangeTests
	|> Test.regFuns ListTests
	|> Test.regFuns TupleTests
	|> Test.regFuns RecordTests
