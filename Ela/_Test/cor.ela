let nested = () -> {
	cout "--start nested";
	yield "nested 1";
	yield "nested 2";
	yield "nested 3";
	cout "--end nested";
};

let c = x -> {
	cout "--start";
	yield "first";
	yield "second";
	yield "third";
	
	for (x in nested())
		yield x;
	
	cout "--end";	 
};

for (x in c())
	cout x;

cout "Finish!";
